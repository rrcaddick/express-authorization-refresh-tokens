{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "urlFilter": "http://localhost:3000/*", // use urlFilter instead of url!
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to Node",
      "type": "node",
      "request": "attach",
      "restart": true
    }
  ]
}
